version = "6.5"
car_deps = []
description = "Ncurses library for text-based user interfaces"

def beforeinst():
    print(":: ncurses is licensed under MIT")

def install():
    import os
    os.system("sudo curl -L https://github.com/redroselinux/car-binary-storage/raw/refs/heads/main/libncursesw.so.6.5 -o /usr/lib/libncursesw.so.6.5")
